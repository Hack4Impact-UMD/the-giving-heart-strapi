{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "eventDate": {
      "type": "date",
      "required": true
    },
    "signUpOpenDate": {
      "type": "date",
      "required": true
    },
    "signUpEndDate": {
      "type": "date",
      "required": true
    },
    "user_attends": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-attend.user-attend",
      "mappedBy": "event"
    },
    "event_roles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-role.event-role",
      "mappedBy": "event"
    }
  }
}
